.container {
  background: url("https://admin.ayjpet.com/attachment//images/applet/22/G5Kdd7u4K75h07H4z7KUK2Pa7k1K1A.png") no-repeat;
  background-size: 100%;
  min-height: 100vh;
  position: relative;
}
.container .shouquan {
  position: fixed;
  width: 100%;
  height: 100vh;
  background: #ffffff;
  z-index: 5000;
  display: flex;
}
.container .shouquan .left {
  flex: 1;
}
.container .shouquan .center {
  flex: 2;
  margin-top: 480rpx;
}
.container .shouquan .center view {
  background: #ffcc33;
  color: black;
  padding: 30rpx;
  border-radius: 50rpx;
  text-align: center;
}
.container .position {
  width: 100%;
  position: absolute;
  bottom: 100rpx;
  left: 0rpx;
  display: flex;
}
.container .position view {
  flex: 1;
  text-align: center;
}
.container .position view image {
  width: 136rpx;
  height: 136rpx;
}
.container .position view text {
  display: block;
  font-size: 26rpx;
  color: #000000;
}
.whiteBox {
  background: white;
  width: 600rpx;
  height: 300rpx;
}
.whiteBox .top {
  margin-top: 50rpx;
  text-align: center;
  font-size: 40rpx;
  font-weight: 600;
  padding-top: 50rpx;
}
.whiteBox .bottom {
  text-align: center;
  margin-top: 30rpx;
}
.whiteBox .bottom .left {
  display: inline-block;
  width: 150rpx;
  margin: 0rpx 20rpx;
  background: #ffcc33;
  padding: 30rpx 30rpx;
  border-radius: 50rpx;
  color: black;
}
