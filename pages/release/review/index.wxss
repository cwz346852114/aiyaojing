.container {
  background: #f2f2f2;
  min-height: 1250rpx;
}
.container .bgPic {
  width: 100%;
  background: url('https://admin.ayjpet.com/attachment//images/applet/22/u4i4AZAKA44hLkaPja4JAAdI4ZKZNJ.png') no-repeat;
  background-size: 100%;
}
.container .bgPic .box {
  overflow: hidden;
  margin-top: 20rpx;
  width: 670rpx;
  margin-left: 20rpx;
  background: white;
  border-radius: 30rpx;
  padding: 30rpx 20rpx;
  margin-bottom: 20rpx;
}
.container .bgPic .box .title {
  font-size: 28rpx;
  padding: 0rpx 10rpx 30rpx;
  color: #000000;
}
.container .bgPic .box .content {
  text-align: center;
  line-height: 88rpx;
  font-size: 24rpx;
  width: 100%;
}
.container .bgPic .box .content view {
  display: inline-block;
  width: 100rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.container .bgPic .box .content view:nth-child(4) {
  margin-right: 0rpx;
}
.container .bgPic .box .content view:nth-child(8) {
  margin-right: 0rpx;
}
.container .bgPic .box .content .bg1 {
  margin-right: 10rpx;
  display: inline-block;
  background: url('https://file.icaohejing.com/Files/Images/20190917/b72c534a35d248b8be0b02c5bba91603.png') no-repeat;
  background-size: 100% 100%;
  width: 160rpx;
  height: 88rpx;
  color: #666666;
}
.container .bgPic .box .content .bg2 {
  margin-right: 10rpx;
  display: inline-block;
  background: url('https://file.icaohejing.com/Files/Images/20190917/5227003cb3a54b8d992fc5a1d4e699e2.png') no-repeat;
  background-size: 100% 100%;
  width: 160rpx;
  height: 88rpx;
  color: #000000;
}
.container .bgPic .box1 {
  width: 710rpx;
  display: inline-block;
  margin-left: 18rpx;
  background: white;
  border-radius: 30rpx;
  overflow: hidden;
  vertical-align: middle;
}
.container .bgPic .box1 image {
  width: 60rpx;
  height: 60rpx;
}
.container .bgPic .box1 view {
  vertical-align: top;
}
.container .bgPic .box2 {
  margin-top: 20rpx;
  width: 650rpx;
  margin-left: 20rpx;
  background: white;
  border-radius: 30rpx;
  overflow: hidden;
  padding: 20rpx 30rpx 0rpx;
}
.container .bgPic .box2 .one {
  display: inline-block;
  font-size: 28rpx;
  color: #666666;
  vertical-align: middle;
  margin-right: 20rpx;
  width: 450rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.container .bgPic .box2 .two {
  font-size: 28rpx;
  color: #000000;
  vertical-align: middle;
  margin-right: 20rpx;
}
.container .bgPic .box2 .fenge {
  height: 1px;
  margin-top: 17rpx;
}
.container .bgPic .box2 .inp1 {
  margin: 0rpx 20rpx;
  width: 100%;
  min-height: 180rpx;
}
.container .bgPic .box2 .img {
  display: inline-block;
  position: relative;
}
.container .bgPic .box2 .img .imgs {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: -13rpx;
  top: -20rpx;
  z-index: 100;
}
.container .bgPic .box2 .img image {
  width: 160rpx;
  height: 160rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
}
.container .bgPic .box2 .img image:nth-child(1) {
  margin-left: 0rpx;
}
.container .bgPic .box2 .img image:nth-child(4) {
  margin-left: 0rpx;
}
.container .bgPic .box2 .img image:nth-child(7) {
  margin-left: 0rpx;
}
.container .bgPic .box2 .video {
  margin-top: 50rpx;
}
.container .bgPic .box2 .video view {
  position: relative;
}
.container .bgPic .box2 .video view .imgs {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: -13rpx;
  top: -20rpx;
  z-index: 100;
}
.container .bgPic .box2 .video view .ved {
  margin-left: 20rpx;
  width: 200rpx;
  height: 200rpx;
}
.container .bgPic .box2 .footer {
  padding-bottom: 36rpx;
  border-bottom: 1px solid #f2f2f2;
}
.container .bgPic .box2 .footer view {
  width: 144rpx;
  height: 144rpx;
  border: 1px dashed #BDBEC4;
  text-align: center;
  display: inline-block;
}
.container .bgPic .box2 .footer view image {
  margin-top: 36rpx;
  width: 44rpx;
  height: 44rpx;
}
.container .bgPic .box2 .footer view text {
  color: #BDBEC4;
  font-size: 22rpx;
  display: block;
}
.container .bgPic .box2 .van-cell {
  padding: 20rpx 0rpx;
}
.container .bgPic .nav {
  display: flex;
}
.container .bgPic .nav view {
  flex: 1;
  font-size: 30rpx;
  font-weight: bold;
  line-height: 76rpx;
  text-align: center;
  margin-bottom: 20rpx;
}
.container .bgPic .nav .bgImg1 {
  width: 266rpx;
  height: 76rpx;
  background: url('https://file.icaohejing.com/Files/Images/20190912/38a10e62d7214d3a9175bfa800c6a027.png') no-repeat center;
  background-size: 100% 100%;
  margin-left: 60rpx;
}
.container .bgPic .nav .bgImg2 {
  width: 262rpx;
  height: 76rpx;
  background: url('https://file.icaohejing.com/Files/Images/20190911/8ece4d2775e648f2a61ab810b1069d8c.png') no-repeat center;
  background-size: 100% 100%;
  margin-left: 32rpx;
}
