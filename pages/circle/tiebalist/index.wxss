.bgImg {
  background: url('https://admin.ayjpet.com/attachment/images/22/2019/09/znm42T3T9cz3ec1Ce3e39aNMDNT32h.png') no-repeat;
  background-size: 100%;
  margin-bottom: 20rpx;
}
.bgImg .head {
  width: 670rpx;
  padding: 20rpx;
  margin-left: 20rpx;
  border-radius: 30rpx;
  background: white;
}
.bgImg .head .top .left {
  display: inline-block;
  vertical-align: top;
}
.bgImg .head .top .left image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 5px;
}
.bgImg .head .top .right {
  display: inline-block;
  margin-left: 14rpx;
}
.bgImg .head .top .right .title {
  font-size: 34rpx;
  font-weight: bold;
}
.bgImg .head .top .right .content {
  margin-top: 7rpx;
  color: #999999;
  font-size: 24rpx;
  width: 512rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.bgImg .head .top .right .bang {
  margin-top: 5rpx;
}
.bgImg .head .top .right .bang text {
  color: #FC4056;
  font-size: 26rpx;
  vertical-align: middle;
}
.bgImg .head .top .right .bang .bang_1 {
  width: 38rpx;
  height: 38rpx;
  vertical-align: middle;
}
.bgImg .head .top .right .bang .bang_2 {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.bgImg .head .top .right .bang .bang_3 {
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
}
.bgImg .head .center {
  padding: 14rpx 0rpx 30rpx 0rpx;
  font-size: 28rpx;
  font-weight: bold;
  display: flex;
  border-bottom: 1px solid #f2f2f2;
}
.bgImg .head .center .left {
  flex: 1;
  margin-top: 16rpx;
}
.bgImg .head .center .zhong {
  flex: 1;
  margin-top: 16rpx;
}
.bgImg .head .center .right {
  flex: 1;
  text-align: right;
  margin-top: 4rpx;
}
.bgImg .head .center .right view {
  display: inline-block;
  width: 140rpx;
  background: #FFE000;
  border-radius: 30rpx;
  text-align: center;
  line-height: 60rpx;
}
.bgImg .head .footer {
  padding: 16rpx 0rpx;
  color: #666666;
  font-size: 26rpx;
  vertical-align: middle;
}
.bgImg .head .footer .zhiding {
  width: 500rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.bgImg .head .footer view {
  padding: 2rpx 0rpx;
}
.bgImg .head .footer .inline {
  margin-left: 420rpx;
  display: inline-block;
}
.bgImg .head .footer image {
  width: 38rpx;
  height: 38rpx;
  vertical-align: middle;
  margin-right: 14rpx;
}
.box {
  padding: 0rpx 20rpx;
  background: white;
  margin-bottom: 20rpx;
}
.box .title {
  width: 100%;
  display: flex;
  padding: 32rpx 0rpx 22rpx 0rpx;
}
.box .title .left {
  flex: 1;
}
.box .title .left image {
  width: 64rpx;
  height: 64rpx;
}
.box .title .center {
  margin-left: 20rpx;
  vertical-align: middle;
  flex: 8;
}
.box .title .center .tian {
  font-size: 26rpx;
}
.box .title .center .xia {
  font-size: 22rpx;
  color: #999999;
}
.box .title .right {
  flex: 1;
  vertical-align: middle;
}
.box .title .right image {
  width: 38rpx;
  margin-top: 15rpx;
  height: 38rpx;
}
.box .contents {
  font-size: 26rpx;
  width: 648rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.box .video {
  width: 200rpx;
  height: 200rpx;
}
.box .imgs {
  margin-top: 20rpx;
}
.box .imgs image {
  width: 200rpx;
  height: 200rpx;
  margin-left: 20rpx;
}
.box .imgs image:nth-child(1) {
  margin-left: 0rpx;
}
.box .imgs image:nth-child(4) {
  margin-left: 0rpx;
}
.box .imgs image:nth-child(7) {
  margin-left: 0rpx;
}
.box .smallBox {
  width: 630rpx;
  padding: 10rpx 20rpx;
  margin-top: 30rpx;
  background: #f2f2f2;
  border-radius: 10rpx;
}
.box .smallBox image {
  width: 120rpx;
  height: 120rpx;
  vertical-align: middle;
}
.box .smallBox text {
  font-size: 26rpx;
  vertical-align: middle;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  width: 400rpx;
  display: inline-block;
}
.box .tag {
  line-height: 30rpx;
  padding: 10rpx;
  background: #FDF5BF;
  display: inline-block;
  border-radius: 30rpx;
  margin: 20rpx 0rpx;
}
.box .tag image {
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.box .tag text {
  display: inline-block;
  vertical-align: middle;
  font-size: 22rpx;
  color: #333333;
  margin-left: 5rpx;
}
.box .shenpin {
  background: #f2f2f2;
  padding: 0rpx 15rpx 15rpx 15rpx;
  border-radius: 20rpx;
  width: 680rpx;
  margin-top: 20rpx;
}
.box .shenpin .header {
  display: flex;
}
.box .shenpin .header view {
  flex: 1;
}
.box .shenpin .header .left image {
  width: 96rpx;
  height: 38rpx;
  vertical-align: middle;
}
.box .shenpin .header .right {
  text-align: right;
}
.box .shenpin .header .right image {
  width: 38rpx;
  vertical-align: middle;
  height: 38rpx;
}
.box .shenpin .header .right text {
  font-size: 24rpx;
  color: #666666;
  margin-left: 8rpx;
  vertical-align: middle;
}
.box .shenpin .footer {
  margin-top: 15rpx;
  font-size: 26rpx;
  color: #333333;
}
.box .di {
  width: 690rpx;
  display: flex;
  padding-bottom: 30rpx;
  margin-top: 34rpx;
}
.box .di image {
  width: 38rpx;
  height: 38rpx;
  vertical-align: middle;
}
.box .di view {
  flex: 1;
}
.box .di text {
  font-size: 24rpx;
  color: #666666;
  margin-left: 8rpx;
  vertical-align: middle;
}
.box .di .left .btn {
  border: none;
  line-height: normal;
}
.box .di .center {
  text-align: center;
}
.box .di .right {
  text-align: center;
}
.fiexd {
  position: fixed;
  right: 50rpx;
  top: 60%;
  width: 108rpx;
  height: 108rpx;
  z-index: 10000;
}
