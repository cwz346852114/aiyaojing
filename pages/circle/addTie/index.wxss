.bgImg {
  background: url('https://admin.ayjpet.com/attachment/images/22/2019/09/znm42T3T9cz3ec1Ce3e39aNMDNT32h.png') no-repeat;
  background-size: 100%;
  margin-bottom: 20rpx;
  width: 100%;
  height: 188rpx;
}
.bgImg .box {
  width: 670rpx;
  padding: 20rpx;
  margin-left: 20rpx;
  border-radius: 30rpx;
  background: white;
}
.bgImg .box .title {
  text-align: right;
}
.bgImg .box .title image {
  width: 107rpx;
  height: 107rpx;
}
.bgImg .box .content .inp {
  min-height: 300rpx;
}
.bgImg .box .video {
  margin-top: 50rpx;
}
.bgImg .box .video view {
  position: relative;
}
.bgImg .box .video view .imgs {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: -13rpx;
  top: -20rpx;
  z-index: 100;
}
.bgImg .box .video view .ved {
  margin-left: 20rpx;
  width: 200rpx;
  height: 200rpx;
}
.bgImg .box .box1 {
  width: 670rpx;
  display: inline-block;
  background: white;
  border-radius: 30rpx;
  overflow: hidden;
  vertical-align: middle;
}
.bgImg .box .box1 image {
  width: 60rpx;
  height: 60rpx;
}
.bgImg .box .box1 view {
  vertical-align: top;
}
.bgImg .box .img {
  margin-left: 20rpx;
  margin-top: 20rpx;
  position: relative;
  display: inline-block;
}
.bgImg .box .img .imgs {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: -13rpx;
  top: -20rpx;
  z-index: 100;
}
.bgImg .box .img image {
  width: 200rpx;
  height: 200rpx;
}
.bgImg .box .img image:nth-child(1) {
  margin: 0rpx;
}
.bgImg .box .img image:nth-child(5) {
  margin: 0rpx;
}
.bgImg .box .img image:nth-child(9) {
  margin: 0rpx;
}
.bgImg .box .smallBox {
  position: relative;
  width: 630rpx;
  padding: 10rpx 20rpx;
  margin-top: 30rpx;
  background: #f2f2f2;
  border-radius: 10rpx;
}
.bgImg .box .smallBox .imgs {
  width: 44rpx;
  position: absolute;
  right: -11rpx;
  top: -22rpx;
  height: 44rpx;
}
.bgImg .box .smallBox image {
  width: 120rpx;
  height: 120rpx;
  vertical-align: middle;
}
.bgImg .box .smallBox text {
  font-size: 26rpx;
  width: 450rpx;
  display: inline-block;
  vertical-align: middle;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.footer {
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  display: flex;
  width: 100%;
  padding: 20rpx 0rpx;
  background: white;
}
.footer view {
  flex: 1;
  text-align: center;
}
.footer view image {
  width: 58rpx;
  height: 58rpx;
}
