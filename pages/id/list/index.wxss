.container {
  background: #f2f2f2;
}
.container .shouquan {
  position: fixed;
  width: 100%;
  height: 100vh;
  background: #ffffff;
  z-index: 5000;
  display: flex;
}
.container .shouquan .left {
  flex: 1;
}
.container .shouquan .center {
  flex: 2;
  margin-top: 480rpx;
}
.container .shouquan .center view {
  background: #ffcc33;
  color: black;
  padding: 30rpx;
  border-radius: 50rpx;
  text-align: center;
}
.container .bgPic {
  width: 100%;
  background: url('https://file.icaohejing.com/Files/Images/20190905/28c5e750166545dab8f1bc4b108e5a50.png') no-repeat;
  background-size: 100%;
}
.container .bgPic .title {
  color: #000000;
  font-size: 32rpx;
  font-weight: 600;
  margin-left: 20rpx;
  padding: 33rpx 0rpx;
}
.container .bgPic .lunbo {
  overflow: hidden;
  padding: 20rpx;
  white-space: nowrap;
  height: 300rpx;
}
.container .bgPic .lunbo .bgImg1 {
  background: url('https://admin.ayjpet.com/attachment//images/applet/22/DIlb4w8OB4gibuIWI4L5u0pLlsO40V.png') no-repeat;
  width: 550rpx;
  height: 280rpx;
  background-size: 100%;
  display: inline-block;
  margin-right: 20rpx;
  line-height: 20rpx;
  border-radius: 30rpx;
}
.container .bgPic .lunbo .bgImg1 .top {
  padding: 20rpx 31rpx 0rpx 31rpx;
  display: flex;
}
.container .bgPic .lunbo .bgImg1 .top .left {
  flex: 1;
}
.container .bgPic .lunbo .bgImg1 .top .left view {
  height: 45rpx;
}
.container .bgPic .lunbo .bgImg1 .top .left view image {
  width: 52rpx;
  height: 40rpx;
  vertical-align: middle;
}
.container .bgPic .lunbo .bgImg1 .top .left view text {
  color: #505D98;
  font-size: 24rpx;
  vertical-align: middle;
  margin-left: 26rpx;
  display: inline-block;
  width: 200rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.container .bgPic .lunbo .bgImg1 .top .right {
  flex: 1;
  text-align: right;
}
.container .bgPic .lunbo .bgImg1 .top .right image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  line-height: 300rpx;
  margin-top: 40rpx;
  margin-right: 40rpx;
  border: 2rpx solid white;
  box-shadow: 10rpx 10rpx 0rpx #757A97;
}
.container .bgPic .lunbo .bgImg1 .bottom {
  margin-top: 35rpx;
  margin-bottom: 35rpx;
}
.container .bgPic .lunbo .bgImg1 .bottom text {
  display: inline-block;
  font-size: 22rpx;
  color: #363F67;
  margin-left: 16rpx;
  vertical-align: middle;
}
.container .bgPic .lunbo .bgImg1 .bottom .imgs {
  width: 32rpx;
  height: 32rpx;
  margin-left: 80rpx;
  vertical-align: middle;
}
